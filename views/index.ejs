<!-- views/index.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>My App</title>
</head>
<body>
    <h1>Welcome to My App</h1>
    <% if (widgets) { %>
        <% widgets.forEach(function(widget) { %>
            <h2><%= widget.Global.Name %></h2>
            <p>Element HTML: <%= widget.elementHtml %></p>
            <p>Model: <%= widget.Global.Model %></p>
            <p>ID: <%= widget.Global.ID %></p>
        <% }); %>
    <% } %>
</body>
</html>